CREATE TABLE data ( 
	num int NOT NULL auto_increment PRIMARY KEY, 
	contents text NULL, 
	date timestamp NULL,
	rw varchar(2) not NULL
);

CREATE TABLE datainfo (
	num int NOT NULL auto_increment PRIMARY KEY,
	db_read int NULL,
	db_write int NULL,
	date timestamp NULL
);

select * from data;

